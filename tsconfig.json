{
	"compilerOptions": {
	  "module": "commonjs",
	  "target": "ES2020",
	  "outDir": "out",
	  "rootDir": "src",
	  "sourceMap": true,
	  "lib": ["ES2020"],
	  "strict": true,
	  "esModuleInterop": true,
	  "skipLibCheck": true,
	  "forceConsistentCasingInFileNames": true
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules", ".vscode-test"]
  }