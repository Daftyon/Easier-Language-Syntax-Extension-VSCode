{
  "name": "easier-language",
  "displayName": "EASIER Language Support",
  "description": "Language support and syntax highlighting for EASIER programming language",
  "version": "0.0.1",
  "publisher": "AHAFDI",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages",
    "Themes"
  ],
  "keywords": [
    "easier",
    "programming language",
    "syntax highlighting"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "easier",
        "aliases": ["EASIER", "easier"],
        "extensions": [".el"],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./icons/easier-file-light.svg",
          "dark": "./icons/easier-file-dark.svg"
        }
      }
    ],
    "grammars": [
      {
        "language": "easier",
        "scopeName": "source.easier",
        "path": "./syntaxes/easier.tmLanguage.json"
      }
    ],
    "iconThemes": [
      {
        "id": "easier-icons",
        "label": "EASIER Language Icons",
        "path": "./themes/easier-icon-theme.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  }
}
